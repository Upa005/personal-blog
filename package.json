{
  "name": "personal-blog",
  "version": "4.6.0",
  "private": true,
  "scripts": {
    "app:local:build": "docker-compose -f docker/compose.local.yml build",
    "app:local:up": "docker-compose -f docker/compose.local.yml up -d",
    "app:local:down": "docker-compose -f docker/compose.local.yml down",
    "app:local:restart": "docker-compose -f docker/compose.local.yml up -d --force-recreate",
    "app:prod:build": "docker-compose -f docker/compose.prod.yml build",
    "app:prod:up": "docker-compose -f docker/compose.prod.yml up -d",
    "app:prod:down": "docker-compose -f docker/compose.prod.yml down",
    "app:prod:restart": "docker-compose -f docker/compose.prod.yml up -d --force-recreate"
  },
  "engines": {
    "node": "6.7.0"
  },
  "dependencies": {
    "body-parser": "^1.16.0",
    "camo": "^0.12.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "express-handlebars": "^3.0.0",
    "http-auth": "~3.1.1",
    "marked": "^0.3.6",
    "moment": "^2.17.1",
    "nedb": "^1.8.0",
    "nodemon": "^1.11.0",
    "rss": "^1.2.2",
    "serve-favicon": "~2.3.0",
    "striptags": "~3.0.0"
  },
  "devDependencies": {
    "css-loader": "^0.26.1",
    "file-loader": "^0.11.1",
    "image-webpack-loader": "^3.1.0",
    "milligram": "^1.2.4",
    "node-sass": "^4.3.0",
    "normalize.css": "^5.0.0",
    "sass-loader": "^4.1.1",
    "script-loader": "^0.7.0",
    "simplemde": "^1.11.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0"
  }
}
